{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "styled-components",
      {
        "displayName": true,
        "fileName": false
      }
    ]
  ]
}
