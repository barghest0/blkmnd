{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./src/*"],
      "reduxStore/*": ["./src/redux/*"],
      "contexts/*": ["./src/context/*"],
      "hocs/*": ["./src/hoc/*"]
    }
  }
}
